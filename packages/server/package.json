{
  "name": "test-bed-geo-fencer-service",
  "version": "0.1.0",
  "description": "Publish notification on KAFKA bus when entities enters of exit geographic areas (conditionally)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bin": "./bin/run.js",
  "scripts": {
    "start-tno-testbed": "node ./dist/src/index.js -- -k driver-testbed.eu:3501 -s driver-testbed.eu:3502",
    "start-local": "node ./dist/src/index.js -- -k localhost:3501 -s localhost:3502 --autoRegisterSchemas",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "tsc-watch --onSuccess \"node ./dist/src/index.js -p 2345 -k localhost:3501 -s localhost:3502 --autoRegisterSchemas\"",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/src/index.js",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "minor-release": "npm version minor && npm publish && git push --follow-tags",
    "test": "echo \"Error: no test specified\" && exit 1",
    "antlr4ts": "antlr4ts -no-listener -visitor -o src/antlr/generated/ -lib src/antlr/ src/antlr/GeoFencerExpression.g4",
    "generate-avro": "cd ./../../schemas && GenerateTypeScriptCode.bat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DRIVER-EU/geo-fencer.git"
  },
  "keywords": [
    "test-bed",
    "Apache",
    "Kafka",
    "geo-fencer",
    "service"
  ],
  "author": "Hein Kluiver <kluiverjh@tno.nl> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DRIVER-EU/geo-fencer/issues"
  },
  "homepage": "https://github.com/DRIVER-EU/geo-fencer#readme",
  "dependencies": {
    "@degordian/avro-to-typescript": "^2.2.3",
    "@nestjs/common": "^6.0.4",
    "@nestjs/core": "^6.0.4",
    "@nestjs/platform-express": "^6.0.4",
    "@nestjs/platform-socket.io": "^6.1.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/websockets": "^6.1.1",
    "@turf/nearest-point-to-line": "^6.0.0",
    "@turf/point-to-line-distance": "^6.0.0",
    "@turf/turf": "^5.1.6",
    "@types/request": "^2.48.1",
    "antlr4ts": "^0.5.0-alpha.3",
    "avsc": "^5.4.11",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.0",
    "nconf": "^0.10.0",
    "node-test-bed-adapter": "^0.7.6",
    "queue": "^6.0.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "socketio": "^1.0.0",
    "swagger-ui-express": "^4.0.2",
    "typescript-task-queue": "^1.0.4",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/command-line-args": "^5.0.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/socket.io": "^2.1.2",
    "@types/underscore": "^1.9.2",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.1.39",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.2.4"
  }
}
